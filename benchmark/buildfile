import LIBS = lib{pulmotor} doctest%lib{doctest} nanobench%lib{nanobench}

for f: cxx{**.benchmark...}
{
	d = $directory($f)
	n = $name($f)...
	./: exe{$n}: cxx{datadef} $f $LIBS
	obj{$n}:
	{
		cxx.poptions += "-I$src_root/../cereal.git/include"
		cxx.poptions += "-I$src_root/../base64_nibbleandhalf"
	}
}

